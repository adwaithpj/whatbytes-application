# Docker Environment Configuration
# Copy this file to .env to use with Docker Compose

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres123@db:5432/healthcare_db

# Django Configuration
SECRET_KEY=django-insecure-docker-development-key-change-in-production
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# PostgreSQL Configuration (for the database container)
POSTGRES_DB=healthcare_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123

# pgAdmin Configuration
PGADMIN_DEFAULT_EMAIL=admin@healthcare.com
PGADMIN_DEFAULT_PASSWORD=admin123
